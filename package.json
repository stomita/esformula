{
  "name": "esformula",
  "version": "1.0.6",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:src && npm run build:flow",
    "build:src": "babel -d lib src",
    "build:flow": "flow-copy-source src lib",
    "test": "ava"
  },
  "dependencies": {
    "acorn": "^5.5.3"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.70.0",
    "flow-copy-source": "^1.3.0"
  },
  "files": [
    "lib",
    "src"
  ],
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ],
    "files": [
      "test/**/*.test.js"
    ]
  }
}
