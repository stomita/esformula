{
  "name": "esformula",
  "version": "1.0.7",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib/*",
    "build": "npm run build:src",
    "build:src": "babel --extensions '.ts' -d lib src",
    "typecheck": "tsc --noEmit",
    "test": "ava"
  },
  "dependencies": {
    "acorn": "^5.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.1",
    "ava": "^0.25.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "files": [
    "lib",
    "src"
  ],
  "ava": {
    "babel": "inherit",
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "@babel/register"
    ],
    "files": [
      "test/**/*.test.ts"
    ]
  }
}
